[user]
	name = Patrick Scheid
	email = p.scheid92@gmail.com
[github]
	user = pscheid92

[push]
	default = simple
[merge]
	tool = meld
[diff]
	tool = meld
[core]
	editor = vim
	pager = less -SRFX
[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
[color]
  ui = auto
[color "branch"]
  current = yellow reverse
  local = yellow
  remote = green
[color "diff"]
  meta = yellow bold
  frag = magenta bold
  old = red bold strike
  new = green bold italic
[color "status"]
  added = yellow
  changed = green
  untracked = cyan
